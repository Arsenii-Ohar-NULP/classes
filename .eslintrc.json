{
    "files": ["**/*.{js,jsx,mjs,cjs,ts,tsx}"],
    "env": {
    "browser": true,
    "es6": true
    },
    "extends": [
    "airbnb-base"
    ],
    "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true
    }
    },
    "rules": {
    "indent": ["error", 4],
    "linebreak-style": "off"
    }
    }
    